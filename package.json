{
    "name": "@openscript/unplugin-favicons",
    "version": "1.1.0-alpha.1",
    "description": "Generate favicons for your project with caching for blazing fast rebuilds.",
    "keywords": [
        "plugin",
        "pwa",
        "vite-plugin",
        "vite",
        "favicon",
        "favicons",
        "image",
        "generate",
        "webpack",
        "rspack",
        "rollup",
        "astro",
        "esbuild",
        "nuxt",
        "sveltekit",
        "svelte",
        "generating-favicons",
        "favicons-generator"
    ],
    "homepage": "https://github.com/anolilab/unplugin-favicons",
    "bugs": {
        "url": "https://github.com/openscript/unplugin-favicons/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/openscript/unplugin-favicons.git"
    },
    "license": "MIT",
    "author": {
        "name": "Robin BÃ¼hler",
        "email": "hi+r@obin.ch",
        "url": "https://r.obin.ch"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "./esbuild": {
            "types": "./dist/esbuild.d.mts",
            "default": "./dist/esbuild.mjs"
        },
        "./nuxt": {
            "types": "./dist/nuxt.d.mts",
            "default": "./dist/nuxt.mjs"
        },
        "./rollup": {
            "types": "./dist/rollup.d.mts",
            "default": "./dist/rollup.mjs"
        },
        "./rspack": {
            "types": "./dist/rspack.d.mts",
            "default": "./dist/rspack.mjs"
        },
        "./vite": {
            "types": "./dist/vite.d.mts",
            "default": "./dist/vite.mjs"
        },
        "./webpack": {
            "types": "./dist/webpack.d.mts",
            "default": "./dist/webpack.mjs"
        },
        "./runtime": {
            "import": {
                "types": "./dist/runtime.d.mts",
                "default": "./dist/runtime.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "module": "dist/index.mjs",
    "types": "dist/index.d.mts",
    "files": [
        "dist/**",
        "CHANGELOG.md",
        "LICENSE.md",
        "SECURITY.md",
        "README.md"
    ],
    "scripts": {
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "lint:eslint": "eslint . --ext js,cjs,mjs,jsx,ts,tsx,json,yaml,yml,md,mdx --max-warnings=0 --config .eslintrc.cjs",
        "lint:eslint:fix": "pnpm run lint:eslint --fix",
        "lint:package-json": "publint",
        "lint:prettier": "prettier --config=.prettierrc.cjs --check .",
        "lint:prettier:fix": "prettier --config=.prettierrc.cjs --write .",
        "lint:secrets": "secretlint **/*",
        "lint:staged": "lint-staged --verbose --concurrent false --debug",
        "lint:text": "textlint ./.github/ ./README.md ./UPGRADE.md --parallel --experimental --cache --dry-run",
        "lint:text:fix": "textlint ./.github/ ./README.md ./UPGRADE.md --parallel --experimental --fix",
        "lint:types": "tsc --noEmit",
        "run:example": "cd example && pnpm exec vite build",
        "sort-package-json": "sort-package-json ./package.json examples/**/package.json",
        "test": "vitest run",
        "test:bench": "vitest bench",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@visulima/find-cache-dir": "^1.0.18",
        "@visulima/package": "^3.2.2",
        "cacache": "^19.0.1",
        "consola": "3.2.3",
        "fast-json-stable-stringify": "^2.1.0",
        "magic-string": "0.30.17",
        "mime": "^4.0.6",
        "mlly": "1.7.3",
        "parse-author": "^2.0.0",
        "parse5": "^7.2.1",
        "ssri": "^12.0.0",
        "unplugin": "^2.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/eslint-parser": "7.25.9",
        "@nuxt/kit": "^3.14.1592",
        "@nuxt/schema": "^3.14.1592",
        "@rspack/core": "^1.1.6",
        "@rspack/plugin-html": "^0.5.8",
        "@sveltejs/kit": "^2.12.2",
        "@tsconfig/esm": "^1.0.5",
        "@tsconfig/node22": "22.0.2",
        "@tsconfig/strictest": "^2.0.5",
        "@types/cacache": "^17.0.2",
        "@types/node": "18.19.70",
        "@types/parse-author": "^2.0.3",
        "@types/parse5": "^7.0.0",
        "@types/ssri": "^7.1.5",
        "@visulima/packem": "^1.10.4",
        "@vitest/coverage-v8": "^2.1.8",
        "caniuse-lite": "^1.0.30001690",
        "esbuild": "^0.24.2",
        "favicons": "^7.2.0",
        "html-webpack-plugin": "^5.6.3",
        "pkg-pr-new": "^0.0.39",
        "prettier": "^3.4.2",
        "publint": "^0.2.12",
        "read-pkg": "9.0.1",
        "rollup": "^4.28.1",
        "secretlint": "9.0.0",
        "semantic-release": "^24.2.0",
        "sort-package-json": "^2.12.0",
        "taze": "^0.18.0",
        "textlint": "^14.4.0",
        "type-fest": "^4.30.2",
        "typescript": "^5.7.2",
        "vite": "^6.0.7",
        "vitest": "^2.1.8",
        "webpack": "^5.97.1"
    },
    "peerDependencies": {
        "@nuxt/kit": "^3",
        "@nuxt/schema": "^3",
        "@rspack/cli": "^1.1.8",
        "esbuild": "*",
        "favicons": "^7.0.2",
        "rollup": "^3.0.1",
        "vite": "^3 || ^4 || ^5 || ^6",
        "webpack": "^4 || ^5"
    },
    "peerDependenciesMeta": {
        "@nuxt/kit": {
            "optional": true
        },
        "@nuxt/schema": {
            "optional": true
        },
        "@rspack/cli": {
            "optional": true
        },
        "@sveltejs/kit": {
            "optional": true
        },
        "esbuild": {
            "optional": true
        },
        "rollup": {
            "optional": true
        },
        "vite": {
            "optional": true
        },
        "webpack": {
            "optional": true
        }
    },
    "packageManager": "pnpm@10.12.1",
    "engines": {
        "node": ">=22.0.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
